{
  "name": "@douglasneuroinformatics/libnest",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.15.3",
  "description": "Generic NestJS decorators, pipes, modules, and utilities used across DNP projects",
  "author": {
    "name": "Douglas Neuroinformatics",
    "email": "support@douglasneuroinformatics.ca"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/DouglasNeuroInformatics/libnest/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DouglasNeuroInformatics/libnest/.git"
  },
  "bugs": {
    "url": "https://github.com/DouglasNeuroInformatics/libnest/issues"
  },
  "exports": {
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./modules": {
      "types": "./dist/modules/index.d.ts",
      "import": "./dist/modules/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --fix src",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.3",
    "@nestjs/core": "^10.2.3",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/testing": "^10.2.5",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.0.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "type-fest": "^4.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "^4.0.0",
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.6",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.21",
    "@vitest/coverage-v8": "^1.3.1",
    "esbuild": "^0.20.1",
    "eslint": "^8.52.0",
    "husky": "^9.0.11",
    "prettier": "^3.0.3",
    "typedoc": "^0.25.11",
    "typescript": "~5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.3.1"
  }
}
