{
  "name": "@douglasneuroinformatics/nestjs-core",
  "version": "0.0.1",
  "description": "Generic NestJS decorators, pipes, modules, and utilities used across DNP projects",
  "license": "LGPL-3.0",
  "type": "module",
  "exports": {
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./modules": {
      "import": "./dist/modules/index.js",
      "types": "./dist/modules/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "types": "./dist/testing/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && pnpm build:js && pnpm build:ts",
    "build:js": "pnpm exec esbuild --bundle --format=esm --outdir=dist --packages=external --platform=node --sourcemap=external  src/core/index.ts src/modules/index.ts src/testing/index.ts",
    "build:ts": "tsc -b tsconfig.build.json",
    "format": "prettier --write src",
    "lint": "tsc && eslint --fix src",
    "push": "pnpm lint && pnpm test && pnpm build && npm publish",
    "test": "vitest run"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.0.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "type-fest": "^4.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "^4.0.0",
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.6",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.21",
    "esbuild": "^0.20.1",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "typescript": "~5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.2.3",
    "@nestjs/core": "^10.2.3",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/testing": "^10.2.5",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  }
}
